<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" xmlns:aapt="http://schemas.android.com/aapt" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item android:state_checked="true">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/vd_col" />
            <corners android:radius="100dp" />
            <size android:width="0dp" android:height="0dp" />
            <stroke android:width="0dp" android:color="@color/vd_col" />
        </shape>
    </item>
    <item android:bottom="5dp" android:left="5dp" android:right="5dp" android:state_checked="false" android:state_enabled="false" android:top="5dp">
        <shape android:dither="true" android:shape="rectangle" android:useLevel="false" android:visible="true">
            <solid android:color="@color/light_vd" />
            <corners android:radius="100dp" />
            <size android:width="13dp" android:height="13dp" />
            <stroke android:width="0dp" android:color="@color/light_vd" />
        </shape>
    </item>
</selector>
